<rrd_graph_def>
     <span>
         <!-- ISO FORMAT: yyyy-MM-dd HH:mm:ss -->
         <start>${startTime}</start>
         <!--  timestamp in seconds is also allowed -->
         <end>${endTime}</end>
     </span>
     <options>
         <anti_aliasing>true</anti_aliasing>
         <title>BI Power Usage</title>
         <title_font>
             <name>Verdana</name>
             <style>BOLD</style>
             <size>14</size>
         </title_font>
         <title_font_color>#FF0000</title_font_color>
         <vertical_label>Power KWH</vertical_label>
     </options>
