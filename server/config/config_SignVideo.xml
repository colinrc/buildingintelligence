<CONFIG lastUpdated="2004-03-18" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="eLife_Config_Schema.xsd">
        <DESC>Demonstration configuration for Sign Video</DESC>
         <!-- ______________________________________  Sign Video DEFS  ____________________  -->
        <DEVICE ACTIVE="Y" DEVICE_TYPE="SIGN_VIDEO" DESCRIPTION="Sign Video Switch">
                <CONNECTION>
                        <SERIAL ACTIVE="N" PORT="/dev/tty.USB Serial" SUPPORTS_CD="N" BAUD="9600" STOP_BITS="1" PARITY="N" DATA_BITS="8" FLOW="RTSCTS"/>
                        <SERIAL ACTIVE="Y" PORT="COM3" SUPPORTS_CD="N" BAUD="1200" STOP_BITS="1" PARITY="N" DATA_BITS="8" FLOW="NONE"/>
                        <IP ACTIVE="N" IP_ADDRESS="127.0.0.1" PORT="5001"/>
                        <IP ACTIVE="N" IP_ADDRESS="192.168.1.70" PORT="4999"/>
                </CONNECTION>
                <CATALOGUE NAME="Sign Video Inputs">
                        <ITEM CODE="cd1" VALUE="1"/>
                        <ITEM CODE="cd2" VALUE="2"/>
                        <ITEM CODE="digital" VALUE="3"/>
                        <ITEM CODE="tv" VALUE="4"/>
                </CATALOGUE>
                <PARAMETERS>
                        <ITEM NAME="AV_INPUTS" VALUE="Sign Video Inputs"/>
                </PARAMETERS>
                <NUVO>
                        <AV KEY="0" DISPLAY_NAME="ALL"/>
                        <AV KEY="2" DISPLAY_NAME="DINING_AV"/>
                        <AV KEY="1" DISPLAY_NAME="KITCHEN_AV"/>
                        <AV KEY="3" DISPLAY_NAME="STUDY_AV"/>
                </NUVO>
        </DEVICE>
        
 
</CONFIG>
