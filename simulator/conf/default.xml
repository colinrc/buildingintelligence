<!--
Text is port 5001
Comfort is port 5002
Raw is port 5003
CBUS is port 5004
GC100 is port 5005
M1 is port 5006
-->

<simulator>

<control>
	<name>Garage Button</name>
	<protocol>COMFORT</protocol>
	<protocol_detail>OUTPUT</protocol_detail>
	<type value="BUTTONS" >
		<key>002</key>
	</type>
</control>

<control>
	<name>Dining Light</name>
	<protocol>CBUS</protocol>
	<type value="BUTTONS">
		<key>11</key>
	</type>
</control>

<control>
	<name>Lounge LV Light</name>
	<protocol>CBUS</protocol>
	<type value="DLT" >
		<key>12</key>
		<label>None</label>
	</type>
</control>

<control>
	<name>Pool Heater</name>
	<protocol>RAW</protocol>
	<type value="BUTTONS_RAW" >
		<key_on>Pool On</key_on>
		<key_off>Pool Off"</key_off>
	</type>
</control>

<control>
	<name>Pool Temp</name>
	<protocol>RAW</protocol>
	<type value="SLIDER_RAW" >
		<key>TEMP</key>
	</type>
</control>

<control>
	<name>Entry PIR (key 2)</name>
	<protocol>M1</protocol>
	<protocol_detail>INPUT</protocol_detail>
	<type value="BUTTONS" >
		<key>002</key>
	</type>
</control>

<control>
	<name>Lawn Sprinkler (key 3)</name>
	<protocol>M1</protocol>
	<protocol_detail>OUTPUT</protocol_detail>
	<type value="BUTTONS" >
		<key>003</key>
	</type>
</control>

<control>
	<name>Garage Door</name>
	<protocol>COMFORT</protocol>
	<protocol_detail>INPUT</protocol_detail>
	<type value="BUTTONS" >
		<key>002</key>
	</type>
</control>

<control>
	<name>Controls</name>
	<protocol>COMFORT</protocol>
	<protocol_detail>CONTROLS</protocol_detail>
	<type value="CONTROLS">
		<key label="Alarm 01">#03AM0103#0A</key>
		<key label="Alarm 02">#03AM02#0A</key>
		<key label="Doorbell">#03DB01#0A</key>
	</type>
</control>

<control>
	<name>Tutondo Src</name>
	<protocol>RAW</protocol>
	<type value="CONTROLS">
		<key label="cd1">!S,52,2,0,1#0A</key>
		<key label="cd2">!S,52,2,0,2#0A</key>
	</type>
</control>


<control>
	<name>Test</name>
	<protocol>GC100</protocol>
	<type value="NONE" />
</control>

</simulator>
