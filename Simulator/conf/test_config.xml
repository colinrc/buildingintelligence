<!--
Text is port 5001
Comfort is port 5002
Raw is port 5003
CBUS is port 5004
GC100 is port 5005
M1 is port 5006
-->

<simulator>

<log hexOnly="Y" />
<!-- All characters will be translated to hex, rather than just unprintable ones. -->

<control>
	<name>Cbus Dining Light</name>
	<protocol>CBUS</protocol>
	<type value="BUTTONS">
		<key value="11" />
	</type>
</control>

<control>
	<name>Cbus Lounge LV Light</name>
	<protocol>CBUS</protocol>
	<type value="SLIDER" >
		<key value="10" />
	</type>
</control>

<control>
	<name>Raw Pool Heater</name>
	<protocol>RAW</protocol>
	<type value="BUTTONS_RAW" >
		<key_on value="Pool On" />
		<key_off value="Pool Off" />
	</type>
</control>

<control>
	<name>Raw Pool Temp</name>
	<protocol>RAW</protocol>
	<type value="SLIDER_RAW" >
		<key value="TEMP" />
	</type>
</control>

<control>
	<name>M1 Entry PIR (key 2)</name>
	<protocol>M1</protocol>
	<protocol_detail>INPUT</protocol_detail>
	<type value="BUTTONS" >
		<key value="002" />
	</type>
</control>

<control>
	<name>M1 Lawn Sprinkler (key 1)</name>
	<protocol>M1</protocol>
	<protocol_detail>OUTPUT</protocol_detail>
	<type value="BUTTONS" >
		<key value="003" />
	</type>
</control>

<control>
	<name>Comfort Garage Door</name>
	<protocol>COMFORT</protocol>
	<protocol_detail>INPUT</protocol_detail>
	<type value="BUTTONS" >
		<key value="002" />
	</type>
</control>


<control>
	<name>Comfort Garage Door</name>
	<protocol>COMFORT</protocol>
	<protocol_detail>CONTROLS</protocol_detail>
	<type value="BUTTONS" >
		<key value="002" />
	</type>
</control>

<control>
	<name>Comfort Controls</name>
	<protocol>COMFORT</protocol>
	<protocol_detail>CONTROLS</protocol_detail>
	<type value="CONTROLS">
		<key value="AM01" Label="Alarm 01" />
		<key value="AM02" Label="Alarm 02" />
		<key value="DB01" Label="Doorbell" />
	</type>
</control>

<control>
	<name>Gc100 test</name>
	<protocol>GC100</protocol>
	<type value="NONE" />
</control>

</simulator>