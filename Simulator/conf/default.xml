<!--
Text is port 5001
Comfort is port 5002
Raw is port 5003
CBUS is port 5004
GC100 is port 5005
M1 is port 5006
-->

<simulator>

<log hexOnly="Y" />
<!-- All characters will be translated to hex, rather than just unprintable ones. -->

<control>
	<name>Cbus Dining Light</name>
	<protocol>CBUS</protocol>
	<type value="BUTTONS">
		<key>11</key>
	</type>
</control>

<control>
	<name>Cbus Lounge LV Light</name>
	<protocol>CBUS</protocol>
	<type value="SLIDER" >
		<key>10</key>
	</type>
</control>

<control>
	<name>Raw Pool Heater</name>
	<protocol>RAW</protocol>
	<type value="BUTTONS_RAW" >
		<key_on>Pool On</key_on>
		<key_off>Pool Off"</key_off>
	</type>
</control>

<control>
	<name>Raw Pool Temp</name>
	<protocol>RAW</protocol>
	<type value="SLIDER_RAW" >
		<key>TEMP</key>
	</type>
</control>

<control>
	<name>M1 Entry PIR (key 2)</name>
	<protocol>M1</protocol>
	<protocol_detail>INPUT</protocol_detail>
	<type value="BUTTONS" >
		<key>002</key>
	</type>
</control>

<control>
	<name>M1 Lawn Sprinkler (key 1)</name>
	<protocol>M1</protocol>
	<protocol_detail>OUTPUT</protocol_detail>
	<type value="BUTTONS" >
		<key>003</key>
	</type>
</control>

<control>
	<name>Comfort Garage Door</name>
	<protocol>COMFORT</protocol>
	<protocol_detail>INPUT</protocol_detail>
	<type value="BUTTONS" >
		<key>002</key>
	</type>
</control>


<control>
	<name>Comfort Garage Door</name>
	<protocol>COMFORT</protocol>
	<protocol_detail>CONTROLS</protocol_detail>
	<type value="BUTTONS" >
		<key>002</key>
	</type>
</control>

<control>
	<name>Comfort Controls</name>
	<protocol>COMFORT</protocol>
	<protocol_detail>CONTROLS</protocol_detail>
	<type value="CONTROLS">
		<key label="Alarm 01">AM01</key>
		<key label="Alarm 02">AM02</key>
		<key label="Doorbell">DB01</key>
	</type>
</control>

<control>
	<name>Gc100 test</name>
	<protocol>GC100</protocol>
	<type value="NONE" />
</control>

</simulator>