                </xs:complexType>
        </xs:element>
        <xs:element name="ANALOGUE">
                <xs:annotation>
                        <xs:documentation>Defines an analog input device</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                        <xs:attribute name="ACTIVE" use="optional">
                                <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                                <xs:enumeration value="N"/>
                                                <xs:enumeration value="Y"/>
                                        </xs:restriction>
                                </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="KEY" use="required">
