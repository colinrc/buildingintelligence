                        <xs:documentation>Describes the string to be displayed on receiving an alert code</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                        <xs:attribute name="ACTIVE" use="optional">
                                <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                                <xs:enumeration value="Y"/>
                                                <xs:enumeration value="N"/>
                                        </xs:restriction>
                                </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="KEY" type="xs:string" use="required"/>
                        <xs:attribute name="DISPLAY_NAME" use="required">
                                <xs:simpleType>
                                        <xs:restriction base="xs:string"/>
