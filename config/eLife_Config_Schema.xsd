                                <xs:simpleType>
                                        <xs:restriction base="xs:string"/>
                                </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="AV_NAME" use="required">
                                <xs:simpleType>
                                        <xs:restriction base="xs:string"/>
