                                        </xs:restriction>
                                </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="PARITY" use="required">
                                <xs:simpleType>
                                        <xs:restriction base="xs:string"/>
                                </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="DATA_BITS" use="required">
                                <xs:simpleType>
                                        <xs:restriction base="xs:byte">
                                                <xs:enumeration value="8"/>
                                                <xs:enumeration value="7"/>
                                        </xs:restriction>
                                </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="FLOW" use="required">
                                <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                                <xs:enumeration value="NONE"/>
