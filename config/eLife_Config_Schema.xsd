                        <xs:choice maxOccurs="unbounded">
                                <xs:element ref="LIGHT_CBUS" minOccurs="0" maxOccurs="unbounded"/>
                                <xs:element ref="SENSOR" minOccurs="0" maxOccurs="unbounded"/>
                        </xs:choice>
                </xs:complexType>
        </xs:element>
        <xs:element name="TUTONDO">
                <xs:complexType>
                        <xs:sequence>
                                <xs:element ref="AUDIO" maxOccurs="unbounded"/>
                        </xs:sequence>
                </xs:complexType>
        </xs:element>
        <xs:element name="KRAMER">
                <xs:complexType>
                        <xs:sequence>
                                <xs:element ref="AV" maxOccurs="unbounded"/>
                        </xs:sequence>
                </xs:complexType>
        </xs:element>
        <xs:element name="PELCO">
                <xs:complexType>
                        <xs:sequence>
                                <xs:element ref="CAMERA" maxOccurs="unbounded"/>
                        </xs:sequence>
                </xs:complexType>
        </xs:element>
        <xs:element name="HAL">
                <xs:annotation>
                        <xs:documentation>Defines a HAL audio system</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                        <xs:sequence>
                                <xs:element ref="AUDIO" maxOccurs="unbounded"/>
                        </xs:sequence>
                </xs:complexType>
        </xs:element>
        <xs:element name="OREGON">
                <xs:annotation>
                        <xs:documentation>Defines an Oregon weather system</xs:documentation>
