                                <xs:element ref="LIGHT_X10"/>
                                <xs:element ref="PULSE_OUTPUT"/>
                                <xs:element ref="TOGGLE_OUTPUT"/>
                                <xs:element ref="ALARM"/>
                                <xs:element ref="ALERT"/>
                                <xs:element ref="ANALOGUE"/>
                                <xs:element ref="TOGGLE_INPUT"/>
                        </xs:choice>
                </xs:complexType>
        </xs:element>
        <xs:element name="DYNALITE">
                <xs:annotation>
                        <xs:documentation>Defines a connection to a Dynalite controller</xs:documentation>
                </xs:annotation>
