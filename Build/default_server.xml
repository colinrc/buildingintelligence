			<IP PORT= "5001" IP_ADDRESS= "172.16.3.120" ACTIVE= "Y"/>
		</CONNECTION>
		<PARAMETERS>
			<ITEM VALUE= "Tutondo Inputs" NAME= "INPUTS"/>
			<ITEM VALUE= "Tutondo Functions" NAME= "FUNCTIONS"/>
		</PARAMETERS>
		<CATALOGUE NAME= "Tutondo Inputs">
